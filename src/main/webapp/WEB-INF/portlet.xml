<?xml version='1.0' encoding='UTF-8' ?>
<portlet-app xmlns='http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd' version='2.0'>
    <portlet>
        <portlet-name>Autodock</portlet-name>
        <display-name>Autodock</display-name>
        <portlet-class>nl.amc.biolab.pgportal.portlets.AutodockVinaPortlet</portlet-class>
        <init-param>
            <name>view-jsp</name>
            <value>/jsp/new_job.jsp</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <portlet-info>
            <title>Autodock Vina</title>
            <short-title>Autodock Vina</short-title>
            <keywords>text</keywords>
        </portlet-info>
    </portlet>
</portlet-app>
